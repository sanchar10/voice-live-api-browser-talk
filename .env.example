# -------------------------------------------------------
# .env.example — Copy this file to .env and fill in your values
# -------------------------------------------------------
# cp .env.example .env        (macOS / Linux)
# copy .env.example .env      (Windows)
# -------------------------------------------------------

# Azure AI Services endpoint (from Azure Portal → your resource → Keys and Endpoint)
AZURE_VOICE_LIVE_ENDPOINT="https://YOUR-RESOURCE.cognitiveservices.azure.com/"

# API key (from Azure Portal → your resource → Keys and Endpoint → Key 1 or Key 2)
AZURE_VOICE_LIVE_API_KEY="YOUR-API-KEY-HERE"

# Model deployment name (use a realtime model for voice)
VOICE_LIVE_MODEL="gpt-realtime-mini"
